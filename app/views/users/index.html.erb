<h1>Listing User</h1>
<%= link_to 'New user', new_user_path %>
<table>
  <tr>
    <th>Title</th>
  </tr>
 
  <% @user.each do |user| %>
    <tr>
      <td><%= user.title %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
      <td><%= link_to 'Edit', edit_user_path(user) %></td>
      <td><%= link_to 'Destroy', user_path(user),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>